{
  "name": "forte",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3456",
    "seo": "node seo.js",
    "build": "yarn seo && next build && next export",
    "analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "*.{js,css,mdx,jsx,html}": "eslint --cache --fix"
  },
  "dependencies": {
    "@builder.io/partytown": "^0.5.4",
    "@mdx-js/loader": "^2.1.1",
    "@next/bundle-analyzer": "^11.1.4",
    "@next/mdx": "^11.1.4",
    "@plaiceholder/next": "^2.3.0",
    "@sergeymyssak/nextjs-sitemap": "^2.0.3",
    "body-scroll-lock": "^4.0.0-beta.0",
    "dotenv": "^10.0.0",
    "framer-motion": "^4.1.17",
    "next": "^12.1.5",
    "next-compose-plugins": "^2.2.1",
    "next-seo": "^4.29.0",
    "plaiceholder": "^2.3.0",
    "preact": "^10.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-responsive-carousel": "^3.2.23",
    "react-slick": "^0.29.0",
    "sass": "^1.51.0",
    "sharp": "^0.29.3",
    "swiper": "^8.1.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.21.1",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "eslint": "^8.14.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^7.0.4",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "windicss-webpack-plugin": "^1.7.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "ignore",
    "insertPragma": false,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false,
    "importOrder": [
      "^@(.*)/(.*)$",
      "^[./]"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:react-hooks/recommended",
      "@antfu/eslint-config",
      "prettier"
    ],
    "env": {
      "es6": true
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "react/prop-types": "off",
      "react/display-name": "off",
      "prettier/prettier": "error",
      "import/order": "off",
      "import/export": "off",
      "antfu/if-newline": "off"
    },
    "ignorePatterns": "*.json"
  },
  "eslintIgnore": [
    ".next",
    ".idea",
    "dist",
    "build",
    "out",
    "node_modules"
  ]
}
